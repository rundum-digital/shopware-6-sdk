{
  "name": "rundum-digital/shopware-6-sdk",
  "description": "API client for Shopware 6",
  "keywords": [
    "swagger",
    "php",
    "sdk",
    "api",
    "shopware"
  ],
  "homepage": "http://swagger.io",
  "license": "proprietary",
  "authors": [
    {
      "name": "Swagger and contributors",
      "homepage": "https://github.com/swagger-api/swagger-codegen"
    }
  ],
  "require": {
    "php": ">=5.5",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "guzzlehttp/guzzle": "^6.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8",
    "squizlabs/php_codesniffer": "~2.6",
    "friendsofphp/php-cs-fixer": "~1.12"
  },
  "autoload": {
    "psr-4": {
      "Rundum\\Shopware6\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Rundum\\Shopware6\\": "test/"
    }
  }
}
